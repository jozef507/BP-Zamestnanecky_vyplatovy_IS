               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></TableView>
         <HBox alignment="CENTER" prefHeight="69.0" prefWidth="810.0" spacing="15.0">
            <children>
               <Button fx:id="add" mnemonicParsing="false" onAction="#btn" prefHeight="40.0" prefWidth="123.0" text="Vyber" />
            </children>
         </HBox>
      </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
   </VBox>
</children>
</AnchorPane>
